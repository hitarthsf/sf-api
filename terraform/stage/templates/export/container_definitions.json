[
  {
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${awslogs_group}",
        "awslogs-region": "${awslogs_region}",
        "awslogs-stream-prefix": "node"
      }
    },
    "environment": [
      {
        "name": "AWS_REGION",
        "value": "${aws_region}"
      }
    ],
    "secrets": [
      {
        "name": "SECRET_KEY_BASE",
        "valueFrom": "${secret_key_base_arn}"
      },
      {
        "name": "DATABASE_URL",
        "valueFrom": "${database_url_arn}"
      }
    ],
    "image": "${image}",
    "essential": true,
    "name": "node"
  }
]
